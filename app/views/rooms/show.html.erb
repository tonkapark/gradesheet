<% title "Room" %>

<p>
  <strong>Building:</strong>
  <%=h @room.building_id %>
</p>
<p>
  <strong>Name:</strong>
  <%=h @room.name %>
</p>
<p>
  <strong>Seats:</strong>
  <%=h @room.seats %>
</p>

<p>
  <%= link_to "Edit", edit_room_path(@room) %> |
  <%= link_to "Destroy", @room, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", rooms_path %>
</p>
